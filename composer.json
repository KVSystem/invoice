{
    "name": "proengeno/invoice",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "symfony/var-dumper": "^4.2",
        "vimeo/psalm": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Proengeno\\Invoice\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Proengeno\\Invoice\\Test\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
